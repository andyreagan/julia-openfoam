    front 
    {
        type empty;
        value uniform 0;
    }
    back 
    {
        type empty;
        value uniform 0;
    }
    topinside 
    {
        type calculated;
        value uniform 0;
    }
    topoutside 
    {
        type calculated;
        value uniform 0;
    }
    bottominside 
    {
        type calculated;
        value uniform 0;
    }
    bottomoutside 
    {
        type calculated;
        value uniform 0;
    }
