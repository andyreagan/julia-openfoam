FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    topinside
    {
        type            fixedFluxPressure;
        rho             rhok;
        gradient        uniform 0;
    }
    topoutside
    {
        type            fixedFluxPressure;
        rho             rhok;
        gradient        uniform 0;
    }
    bottominside
    {
        type            fixedFluxPressure;
        rho             rhok;
        gradient        uniform 0;
    }
    bottomoutside
    {
        type            fixedFluxPressure;
        rho             rhok;
        gradient        uniform 0;
    }
}


